<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.leonardoperez.v3.Menuprincipal"
    >


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descarga Uno"
        android:id="@+id/BT_Descarga_Uno"
        android:onClick="Descarga_FDA1"
        android:drawableLeft="@mipmap/gota2"
        android:drawableRight="@mipmap/gota2"
        android:layout_alignWithParentIfMissing="false"
        android:gravity="left|center_vertical"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ellas"

        android:layout_above="@+id/BT_Descarga_Uno">

    </LinearLayout>


</RelativeLayout>
